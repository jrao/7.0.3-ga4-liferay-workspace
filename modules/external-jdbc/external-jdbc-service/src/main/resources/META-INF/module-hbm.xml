<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="org.javasavvy.demo.model.Country" />
	<class name="org.javasavvy.demo.model.impl.CountryImpl" table="country">
		<id name="countryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="countryName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>